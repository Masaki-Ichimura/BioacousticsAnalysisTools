<PreprocessedIconButton@MDIconButton>:
    icon_size: '20sp'

<PreprocessedTab>:
    title: ''
    icon: 'format-list-bulleted'

    BoxLayout:
        orientation: 'horizontal'
        pos: root.pos
        size: root.size

        BoxLayout:
            orientation: 'vertical'
            size_hint_x: 19/60

            MDBoxLayout:
                id: preprocessed_bar
                orientation: 'horizontal'
                padding: dp(5)
                adaptive_height: True

                PreprocessedIconButton:
                    icon: 'file-remove-outline'
                    on_press: root.remove_button_clicked()

                PreprocessedIconButton:
                    icon: 'delete-forever-outline'
                    on_press: root.reset_button_clicked()

                PreprocessedIconButton:
                    icon: 'sort-alphabetical-ascending'
                    on_press: root.sort_button_clicked()

            HSeparator:
                rgba: 0.5, 0.5, 0.5, 1

            ScrollView:
                do_scroll_x: False

                ScrollableTreeView:
                    id: audio_treeview
                    root_options: {'text': 'preprocessed audio data', 'font_size': '15sp'}

        VSeparator:

        Button:
            size_hint_x: 41/60
            text: '- 各信号のプロット？\n- 保存機能'
            on_press: print(dir(self))
            disabled: True
